<Window x:Name="wndSettings" x:Class="Clicker.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
				xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				xmlns:local="clr-namespace:Clicker"
        Title="Settings" Height="300" Width="300" WindowStyle="ToolWindow" Loaded="wndSettings_Loaded" Icon="Media/gearYellow.ico">
	<Window.Resources>
		<local:Config x:Key="config"/>
	</Window.Resources>
	<Grid>
		<xctk:PropertyGrid x:Name="propSettings" AutoGenerateProperties="True">
			<xctk:PropertyGrid.PropertyDefinitions>
				<xctk:PropertyDefinition Name="RunAsService"/>
			</xctk:PropertyGrid.PropertyDefinitions>
		</xctk:PropertyGrid>

	</Grid>
</Window>
